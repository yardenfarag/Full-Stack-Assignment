{
  "name": "data-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --platform=node --format=esm --target=es2022 --external:fastify --external:@fastify/cors --outfile=dist/server.mjs",
    "dev": "node dist/server.mjs",
    "dev:src": "tsx watch src/index.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "fastify": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
